{
  "name": "MFF",
  "version": "1.0.0",
  "description": "Front-end package management for the open mff theme based on Open Y carnation.",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "bootstrap": "^4.4.1",
    "breakpoint-sass": "^2.7.1",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "popper.js": "^1.16.0",
    "sass": "^1.53.0",
    "sass-lint": "^1.13.1",
    "sass-lint-webpack": "^1.0.2",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.1.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^1.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.9",
    "webpack-glob-entry": "^2.1.1",
    "webpack-log": "^3.0.1",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "build:scss": "node-sass -r src/scss/style.scss -o dist/css/",
    "build": "webpack --config webpack.build.js --debug --output-pathinfo",
    "dev": "webpack --config webpack.dev.js --info-verbosity verbose",
    "watch:scss": "npm run build:scss && node-sass --watch --source-map-embed 'src/scss/style.scss' 'dist/css/style.css'"
  }
}
